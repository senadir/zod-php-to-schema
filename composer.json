{
    "name": "senadir/zod-php-to-schema",
    "description": "Convert Zod-like PHP objects to JSON Schema definitions",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Nadir Seghir",
            "email": "nadir.seghir@gmail.com",
            "homepage": "https://nadir.blog"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zod2Schema\\Zod2Schema\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zod2Schema\\Zod2Schema\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.11",
        "wp-coding-standards/wpcs": "^3.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "phpcompatibility/phpcompatibility-wp": "^2.1",
        "phpcompatibility/php-compatibility": "^9.3",
        "captainhook/captainhook": "^5.24"
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "keywords": [
        "zod",
        "json-schema",
        "validation",
        "schema"
    ],
    "homepage": "https://github.com/senadir/zod-php-to-schema",
    "support": {
        "issues": "https://github.com/senadir/zod-php-to-schema/issues",
        "source": "https://github.com/senadir/zod-php-to-schema"
    },
    "minimum-stability": "stable"
}
